{% extends 'base.html.twig' %}

{% block title %}Hello toi!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Formulaire pour ajouter une entreprise :</h1>
  
{{ form_start(unFormulaire) }}


{{ form_row(unFormulaire.nom)}}
{{ form_row(unFormulaire.adresse)}}
{{ form_row(unFormulaire.activite)}}
{{ form_row(unFormulaire.siteWeb)}}

<button type="submit" class="btn btn-dark">Enregistrer</button>
{{ form_end(unFormulaire) }}
</div>

{% endblock %}